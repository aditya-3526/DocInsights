{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://docinsights-api-production.up.railway.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite"
}